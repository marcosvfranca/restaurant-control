{
  "name": "restaurant-control",
  "version": "1.0.0",
  "description": "Restaurant Control API",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev --transpile-only src/index.ts | pino-pretty -c",
    "test": "yarn lint && yarn style:check && yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check ./src/**/*.ts ./test/**/*.ts",
    "style:fix": "prettier --write ./src/**/*.ts ./test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcosvfranca/restaurant-control.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Marcos Fran√ßa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcosvfranca/restaurant-control/issues"
  },
  "homepage": "https://github.com/marcosvfranca/restaurant-control#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "5.7.22",
    "@types/node": "^14.14.25",
    "@types/pino": "^6.3.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "pino-pretty": "^4.5.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "5.9.18",
    "pino": "^6.11.1"
  }
}
